<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.RoleEntity" table="Role_" schema="dbo" catalog="JobService">
        <id name="idRole">
            <column name="idRole" sql-type="int"/>
            <generator class="identity"/>
        </id>
        <property name="rolename">
            <column name="rolename" sql-type="nvarchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>